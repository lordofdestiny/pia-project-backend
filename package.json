    "mime-types": "^2.1.35",
    "mongodb": "^4.5.0",
    "mongoose": "^6.3.3",
    "mongoose-lean-virtuals": "^0.9.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nanoid": "^3.3.6",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0"
