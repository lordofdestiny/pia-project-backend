    "tsconfig-paths": "^4.2.0",
    "tslint": "^5.11.0",
    "typescript": "^4.0.1"
  },
  "_moduleAliases": {
    "@controllers": "dist/controllers",
    "@middleware": "dist/middleware",
    "@models": "dist/models",
    "@routes": "dist/routes",
    "@utils": "dist/utils"
  },
  "dependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.7",
