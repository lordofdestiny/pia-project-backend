{
  "name": "pia-projekat-backend",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "postbuild": "tsc-alias && copyfiles -u 1 src/templates/*.html dist/",
    "start": "node dist/server.js",
    "dev": "node -r dotenv/config -r module-alias/register dist/server.js",
    "predev": "npm run build",
    "watch-node": "nodemon -r dotenv/config -r tsconfig-paths/register --inspect src/server.ts",
    "watch-ts": "tsc -w",
    "pretty": "prettier --write \"./**/*.{js,jsx,ts,tsx,json}\""
  },
  "author": "lordofdestiny",
  "license": "ISC",
  "_moduleAliases": {
    "@controllers": "dist/controllers",
    "@middleware": "dist/middleware",
    "@models": "dist/models",
    "@routes": "dist/routes",
    "@utils": "dist/utils"
  },
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "dotenv": "^16.3.1",
    "module-alias": "^2.2.3",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.1",
    "preview-email": "^3.0.19",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "tslint": "^5.11.0",
    "typescript": "^4.0.1"
  },
  "dependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.7",
    "@types/luxon": "^3.3.2",
    "@types/mime-types": "^2.1.1",
    "@types/mongodb": "^4.0.7",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.32",
    "@types/node-fetch": "^2.6.4",
    "@types/nodemailer": "^6.4.10",
    "@types/passport": "^1.0.12",
    "@types/passport-local": "^1.0.35",
    "@types/preview-email": "^3.0.1",
    "@types/qrcode": "^1.5.2",
    "bcrypt": "^5.1.0",
    "connect-mongo": "4.6",
    "cors": "^2.8.5",
    "envfile": "^6.18.0",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "luxon": "^3.4.3",
    "mime-types": "^2.1.35",
    "mongodb": "^4.5.0",
    "mongoose": "^6.5.0",
    "mongoose-lean-virtuals": "^0.9.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nanoid": "^3.3.6",
    "nodemailer": "6.9.9",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "pdf-creator-node": "^2.3.5",
    "qrcode": "^1.5.3"
  }
}
